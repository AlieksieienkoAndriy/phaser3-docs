{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": "../phaser/src/gameobjects/blitter",
        "exclude": [ "../phaser/src/physics/matter-js/lib", "../phaser/src/polyfills" ],
        "includePattern": ".+\\.js?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "debug": true,
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./out/",
        "outputSourceFiles": false,
        "outputSourcePath": true,
        "recurse": true
    }
}