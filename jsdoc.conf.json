{
    "tags": {
        "allowUnknownTags": true
    },
    "markdown"  : {
        "parser"   : "gfm",
        "hardwrap" : true
    },
    "source": {
        "include": [
            "../phaser/src/phaser.js",
            "../phaser/src/textures/"
        ],
        "gos": [
            "../phaser/src/gameobjects/index.js",
            "../phaser/src/gameobjects/components/",
            "../phaser/src/gameobjects/GameObject.js",
            "../phaser/src/gameobjects/GameObjectCreator.js",
            "../phaser/src/gameobjects/GameObjectFactory.js",
            "../phaser/src/gameobjects/bitmaptext/",
            "../phaser/src/gameobjects/blitter/",
            "../phaser/src/gameobjects/graphics/",
            "../phaser/src/gameobjects/image/",
            "../phaser/src/gameobjects/mesh/",
            "../phaser/src/gameobjects/particles/",
            "../phaser/src/gameobjects/pathfollower/",
            "../phaser/src/gameobjects/quad/",
            "../phaser/src/gameobjects/sprite/",
            "../phaser/src/gameobjects/sprite3d/",
            "../phaser/src/gameobjects/text/",
            "../phaser/src/gameobjects/tilesprite/",
            "../phaser/src/gameobjects/zone/"
        ],
        "done": [
            "../phaser/src/actions/",
            "../phaser/src/math/",
            "../phaser/src/utils/",
            "../phaser/src/geom/",
            "../phaser/src/animations/",
            "../phaser/src/math/",
            "../phaser/src/tilemaps/",
            ""
        ],
        "exclude": [
            "../phaser/src/tilemaps/parsers",
            "../phaser/src/physics/matter-js/lib",
            "../phaser/src/polyfills"
        ],
        "includePattern": ".+\\.js?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "jsdoc-plugins/typedef"
    ],
    "templates": {
        "systemName": "Phaser",
        "systemSummary": "HTML5 Game Framework",
        "footer": "footer",
        "copyright": "Phaser Copyright Â© 2018 Photon Storm Ltd and the contributors to the Phaser projects.",
        "includeDate": true
    },
    "opts": {
        "debug": true,
        "destination": "./out/",
        "encoding": "utf8",
        "outputSourceFiles": false,
        "outputSourcePath": true,
        "recurse": true,
        "private": false,
        "lenient": true,
        "sourceType": "script",
        "template": "node_modules/foodoc/template"
    }
}